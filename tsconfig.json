{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es5",
    "strictNullChecks": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@ukis/base-layers-raster": [
        "projects/base-layers-raster/src/public_api"
      ],
      "@ukis/base-layers-raster/*": [
        "projects/base-layers-raster/*"
      ],
      "@ukis/map-ol": [
        "projects/map-ol/src/public_api"
      ],
      "@ukis/map-ol/*": [
        "projects/map-ol/*"
      ],
      "@ukis/services-layers": [
        "projects/services-layers/src/public_api"
      ],
      "@ukis/services-layers/*": [
        "projects/services-layers/*"
      ],
      "@ukis/services-map-state": [
        "projects/services-map-state/src/public_api"
      ],
      "@ukis/services-map-state/*": [
        "projects/services-map-state/*"
      ],



      "@ukis/layer-control": [
        "projects/layer-control/src/public_api"
      ],
      "@ukis/layer-control/*": [
        "projects/layer-control/*"
      ],
      "screenshot": [
        "dist/screenshot"
      ],
      "screenshot/*": [
        "dist/screenshot/*"
      ],
      "sld-parser": [
        "dist/sld-parser"
      ],
      "sld-parser/*": [
        "dist/sld-parser/*"
      ],
      "services-wps": [
        "dist/services-wps"
      ],
      "services-wps/*": [
        "dist/services-wps/*"
      ],
      "translate": [
        "dist/translate"
      ],
      "translate/*": [
        "dist/translate/*"
      ]
    }
  }
}