{
  "compileOnSave": false,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "strictNullChecks": false,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@ukis/*": [
        "dist/*",
        "projects/*",
        "node_modules/@dlr-eoc/*",
        "frontend-libraries/projects/*"
      ],
      "@dlr-eoc/*": [
        "dist/*",
        "projects/*",
        "node_modules/@dlr-eoc/*",
        "frontend-libraries/projects/*"
      ],
      "helpers": [
        "dist/helpers"
      ],
      "helpers/*": [
        "dist/helpers/*"
      ]
    }
  }
}